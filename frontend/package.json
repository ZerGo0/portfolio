{
  "name": "portfolio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:warnings": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --threshold warning",
    "format": "prettier --write . --cache",
    "lint": "prettier --write . --cache | { grep -v '(unchanged)' || true; } && eslint . --cache",
    "lint:warnings": "eslint . --cache --max-warnings 0",
    "deploy:production": "wrangler deploy",
    "preview": "wrangler dev"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250718.0",
    "@iconify-json/carbon": "^1.1.37",
    "@riadh-adrani/utils": "1.0.0",
    "@sveltejs/adapter-auto": "3.2.4",
    "@sveltejs/adapter-cloudflare": "^7.1.0",
    "@sveltejs/kit": "^2.25.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/dompurify": "^3.0.5",
    "@types/marked": "^6.0.0",
    "@types/prismjs": "^1.26.4",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@unocss/core": "^0.62.2",
    "@unocss/extractor-svelte": "^0.62.3",
    "@unocss/preset-icons": "^0.62.2",
    "@unocss/svelte-scoped": "^0.62.3",
    "dayjs": "^1.11.13",
    "dompurify": "^3.1.6",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^9.1.0",
    "marked": "^14.0.0",
    "marked-gfm-heading-id": "^4.0.1",
    "marked-mangle": "^1.1.9",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prismjs": "^1.29.0",
    "sass": "^1.77.8",
    "slugify": "^1.6.6",
    "svelte": "^5.36.7",
    "svelte-check": "^4.3.0",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "unocss": "^0.62.2",
    "vite": "^5.4.4",
    "wrangler": "^4.25.0"
  },
  "type": "module",
  "dependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.31.0",
    "eslint-plugin-svelte": "^3.11.0",
    "globals": "^16.3.0",
    "typescript-eslint": "^8.37.0"
  }
}
